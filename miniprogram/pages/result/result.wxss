/* pages/result/result.wxss */
.result{
  width: 100vw;
  height: 100vh;
  background: rgb(249,249,249);
}
.result-img-box{
  padding-top: 18vh;
  text-align: center;
}
.result-img{
  width: 200rpx;
}
.result-des{
  display: inline-block;
  width: 100%;
  text-align: center;
  margin-top: 50rpx;
}
.result-reason{
 display: inline-block;
  width: 100%;
  text-align: center;
  margin-top: 50rpx;
  font-size: 20rpx;
  color: rgb(176,177,178);
}
.result-again-btn{
  width: 100%;
  height: 80rpx;
  border: 1rpx solid rgb(176,177,178);
  text-align: center;
  font-size: 25rpx;
  margin-top: 60rpx;
  border-radius: 10rpx;
  margin-bottom: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: 400;
  color: rgb(121,122,123);
}
